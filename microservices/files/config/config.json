{
  "rabbit": {
    "url": "amqp://guest:guest@localhost:5672/"
  },
  "mongoose": {
    "uri": "mongodb://localhost:27017/files"
  },
  "adminTemplate": "/admin/",
  "template": "index.njk",
  "upload": {
    "config": {
      "upload": true,
      "path": "/images/",
      "limits": {
        "fileSize": "100 * 1024 * 1024"
      },
      "mimeTypeLimit": [
        "image/jpeg",
        "image/jpg",
        "image/png",
        "image/JPG"
      ]
    },
    "folder": "/images/"
  }
}